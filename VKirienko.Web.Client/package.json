{
  "name": "vkirienko-site",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "prestart": "node aspnetcore-https",
    "start": "run-script-os",
    "start:windows": "ng serve --ssl --ssl-cert \"%APPDATA%\\ASP.NET\\https\\%npm_package_name%.pem\" --ssl-key \"%APPDATA%\\ASP.NET\\https\\%npm_package_name%.key\" --host=127.0.0.1",
    "start:default": "ng serve --ssl --ssl-cert \"$HOME/.aspnet/https/${npm_package_name}.pem\" --ssl-key \"$HOME/.aspnet/https/${npm_package_name}.key\" --host=127.0.0.1",
    "build": "ng build",
    "postbuild": "imagemin src/assets/img/* --plugin.webp.quality=100 --out-dir=dist/VKirienko.Web.Client/assets/img-min/",
    "eslint": "eslint \"src/**/*.ts\" --fix",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && ng serve -o",
    "ncu": "npm-check-updates -u"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "21.0.1",
    "@angular/cdk": "21.0.1",
    "@angular/common": "21.0.1",
    "@angular/compiler": "21.0.1",
    "@angular/core": "21.0.7",
    "@angular/forms": "21.0.1",
    "@angular/material": "^21.0.1",
    "@angular/platform-browser": "21.0.1",
    "@angular/platform-browser-dynamic": "21.0.1",
    "@angular/platform-server": "21.0.1",
    "@angular/router": "21.0.1",
    "@fortawesome/angular-fontawesome": "^4.0.0",
    "@fortawesome/fontawesome-free": "^7.1.0",
    "@fortawesome/fontawesome-svg-core": "^7.1.0",
    "@fortawesome/free-brands-svg-icons": "^7.1.0",
    "@fortawesome/free-solid-svg-icons": "^7.1.0",
    "@microsoft/applicationinsights-web": "^3.3.10",
    "@microsoft/signalr": "^10.0.0",
    "@ngneat/until-destroy": "^10.0.0",
    "arrive": "^2.5.2",
    "bootstrap": "4.6.2",
    "bootstrap-material-design": "4.1.3",
    "bootstrap-notify": "3.1.3",
    "chartist": "1.5.0",
    "googleapis": "166.0.0",
    "hammerjs": "2.0.8",
    "jquery": "3.7.1",
    "moment": "2.30.1",
    "perfect-scrollbar": "1.5.6",
    "popper.js": "1.16.1",
    "rxjs": "^7.8.2",
    "zone.js": "~0.16.0"
  },
  "devDependencies": {
    "@angular-devkit/schematics": "^21.0.1",
    "@angular/build": "^21.0.1",
    "@angular/cli": "^21.0.1",
    "@angular/compiler-cli": "21.0.1",
    "@angular/language-service": "21.0.1",
    "@eslint/config-inspector": "^1.4.2",
    "@eslint/js": "^9.39.1",
    "@types/bootstrap": "5.2.10",
    "@types/google.maps": "3.58.1",
    "@types/jquery": "3.5.33",
    "@types/node": "^24.10.1",
    "imagemin": "^9.0.1",
    "imagemin-cli": "^8.0.0",
    "imagemin-webp": "^8.0.0",
    "npm-check-updates": "^19.1.2",
    "run-script-os": "^1.1.6",
    "ts-node": "10.9.2",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.48.0"
  },
  "overrides": {
    "autoprefixer": "10.4.5"
  },
  "browser": {
    "events": false,
    "http": false,
    "https": false,
    "url": false,
    "util": false
  }
}
